gctl:en 
    a gctl:Language ;
    gctl:description """
        With the goal of unifying categories for all Christian media so that christians all over the world can search for media specific to their needs.

        [Read more about this iniative](/about)
    """ ;
    rdfs:label "English" ;
    gctl:welcome """
    Welcome to the 
    
    __Global Christian Taxonomy__
    """ ;
    gctl:title "Global Christian Taxonomy" ;
    gctl:background unsplash:1544640808-32ca72ac7f37 ;
    gctl:translation-identifier "Identifier"@en ;
    gctl:translation-other-translations "Other translations"@en ;
    gctl:translation-search-words "Search words"@en ;
    gctl:translation-all "All"@en ;
    gctl:bcp47 "en".

gctl:nl 
    a gctl:Language ;
    gctl:welcome """
    Welkom tot de

    __Globale Christelijke Taxonomie__
    """ ;
    gctl:description """

        Een categorie systeem voor alle christelijke media zo dat christenen overal kunnen zoeken naar media specifiek voor hun.

        [Lees meer over dit initiatief](/about)
    """ ;
    gctl:title "Globale Christelijke Taxonomie" ;
    rdfs:label "Nederlands" ;
    gctl:translation-identifier "Identificatie"@nl ;
    gctl:translation-other-translations "Andere vertalingen"@nl ;
    gctl:translation-search-words "Zoekwoorden"@nl ;
    gctl:translation-all "Overzicht"@nl ;
    gctl:bcp47 "nl".
